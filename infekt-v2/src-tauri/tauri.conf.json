{
  "productName": "iNFekt",
  "version": "2.0.0",
  "identifier": "org.syndicode.infekt-v2",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "iNFekt v2",
        "fullscreen": false,
        "resizable": true,
        "center": true,
        "minWidth": 550,
        "minHeight": 200,
        "width": 750,
        "height": 800
      }
    ],
    "withGlobalTauri": false,
    "macOSPrivateApi": false,
    "trayIcon": {
      "iconPath": "icons/iNFekt.ico"
    }
  },
  "bundle": {
    "active": false,
    "targets": "all",
    "icon": [
      "icons/iNFekt.ico",
      "icons/iNFekt_6_256x256x32.png",
      "icons/iNFekt_8_32x32x32.png",
      "icons/iNFekt_9_16x16x32.png"
    ]
  }
}
